command /james:
	permission: *
	trigger:
		spawn 1 iron golem at location of player
		set name of last spawned iron golem to "&6&lJ&eames"
		stop
	
	
	
	
	
	
	
	
on chat:
	if player is in world "Spawn":

		if message contains "James Kaffe":
			wait 1 second
			send "&6&lJ&eames &8&l▶&7 Kaffe ist unterwegs" to all players in world
			stop
			
		else if message contains "James wie gehts dir":
			wait 1 second
			send "&6&lJ&eames &8&l▶&7 Mir geht es soweit ganz gut" to all players in world
			stop
			
		else if message contains "James ist geil":
			wait 1 second
