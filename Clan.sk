								delete {clan::%loop-player%.created}
								delete {clan::%loop-player%.invited}
								delete {clan::%loop-player%.input}
								delete {clan::%loop-player%.inviter}
						delete {clan::list::%{clan::%player%.in}%}
						delete {clan::%{clan::%player%.in}%.players::*}
						delete {clan::%player%.invited}
						delete {clan::%{clan::%player%.in}%.Members}
						delete {clan::%player%.input}
						loop {clan::list::*}:
							if loop-value is {clan::%player%.in}:
								delete {clan::list::%loop-index%}
								exit loop
						delete {clan::%player%.inviter}
						delete {clan::%player%.status}
						delete {clan::%player%.created}
						delete {clan::%{clan::%player%.in}%.Taler}
						delete {clan::%{clan::%player%.in}%.slots}
						delete {clan::%{clan::%player%.in}%.Members}
						delete {clan::%{clan::%player%.in}%.level}
						delete {clan::%{clan::%player%.in}%.owner}
						loop {clan::%{clan::%player%.in}%.players::*}:
							delete {clan::%loop-value%.in}
						delete {clan::%player%.in}
						delete {clan::%{clan::%player%.in}%.players::*}
						delete {clan::%player%.delete}
