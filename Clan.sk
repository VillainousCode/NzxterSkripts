						broadcast ""
						broadcast "&7Der Clan &e%{clan::%player%.in}% &7wurde aufgelöst!"
						broadcast ""
						broadcast "          &8&m--&7&m--&e&m--&6&m--&8( &6&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
					
						
						remove {clan::%player%.in} from {clan::list::*}
						delete {clan::%player%.created}
						delete {clan::%player%.tag}
						delete {clan::%{clan::%player%.in}%.Motto}
						delete {%{clan::%player%.in}%.Equip::*}
						delete {clan::%{clan::%player%.in}%.Taler}
						delete {rank6.name.%{clan::%player%.in}%}
						delete {rank5.name.%{clan::%player%.in}%}
						delete {rank4.name.%{clan::%player%.in}%}
						delete {rank3.name.%{clan::%player%.in}%}
						delete {rank2.name.%{clan::%player%.in}%}
						delete {rank1.name.%{clan::%player%.in}%}
