variables:
	{clan::%player%.in} = "&eClanlos"

options:
	Prefix: &8&l【&e&lClan&8&l】&8&l▶&7
	
on join:
	wait 1 tick
	{clan::%{clan::%player%.in}%.owner} is player:
		{clan::%{clan::%player%.in}%.presres} is not set:
			set {clan::%{clan::%player%.in}%.prestige} to 0
			set {clan::%{clan::%player%.in}%.prestige.pay} to 1000000
			set {clan::%{clan::%player%.in}%.presres} to true

#on join:
#	{clan::%player%.in} is set:
#		make console execute command "ne suffix %name of player% &8┃ &7%{clan::%player%.in}%"

	
command /setleader [<text>] [<player>]:
	trigger:
		player is "Clan":
			arg-1 is not set:
				send "&e/setleader <Clan> <Spieler>"
				stop
			if {clan::%arg-1%.owner} is not set:
				send "{@Prefix} &cDieser Clan existiert nicht!"
				stop
			if {clan::%arg player%.created} is true:
				send "{@Prefix} &cDer Spieler ist schon ein Leader.!"
				stop
			set {clan::%arg-1%.owner} to arg player
			set {clan::%arg-2%.status} to true
			set {clan::%arg-2%.in} to arg-1
			set {clan::%arg-1%.Taler} to 0
			broadcast "{@Prefix} Spieler &f%arg player% &7wurde zum neuen Leader des Clans &e%arg-1% &7ernannt!"
			stop
	
command /clan [<text>] [<text>] [<text>] [<text>]:
	aliases: /club, /guild, /guilde, /gilde
	trigger:
		if arg-1 is not set:
			make player execute command "/clan 1"
			stop
			
		arg-1 is "1":	
			send "          &8&m--&7&m--&e&m--&6&m--&8( &e&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
			send " "
			send " &8▪ &e/clan liste &8&l» &7Zeigt alle Clans ab Level 4"
			send " &8▪ &e/clan erstellen &f<Clan-Name> &8&l» &7Erstellt einen Clan &8(&a1500 Taler&8)"
			send " &8▪ &e/clan einladen &f<Spieler> &8&l» &7Lädt Spieler in dein Clan"
			send " &8▪ &e/clan verlassen &8&l» &7Verlässt deinen Momentanigen Clan"
			send " &8▪ &e/clan kasse einzahlen &f<Taler> &8&l» &7Spendet dem Clan Taler!"
			send " &8▪ &e/clan info &8&l» &7Zeigt Infos über deinen Clan!"
			send " &8▪ &e/clan kasse &8&l» &7Zeigt Infos über deine Clankasse!"
			send " &8▪ &e/clan promote &f<Spieler> &8&l» &7Promote einen Clanmitglied!"
			send " &8▪ &e/clan demote &f<Spieler> &8&l» &7Demote ein Clanmitglied!"
			send " &8▪ &e/clan 2 &8&l» &cSeite 2"
			send " "		
			send "          &8&m--&7&m--&e&m--&6&m--&8( &e&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
			stop			
			
			
		arg-1 is "2":	
			send "          &8&m--&7&m--&e&m--&6&m--&8( &e&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
			send " "		
			send " &8▪ &e/clan base setzen &8&l» &7Setzt die Base vom Clan."
			send " &8▪ &e/clan base &8&l» &7Das bringt dich zur Clanbase!"
			send " &8▪ &e/clan auflösen &8&l» &7Der Clan wird gelöst."
			send " &8▪ &e/clan motto &f<Motto> &8&l» &7Das setzt das Motto des Clans."
			send " &8▪ &e/clan kicken &f<name> &8&l» &7Kickt Spieler vom Clan!"
			send " &8▪ &e/clan prestige &f<name> &8&l» &7Zeige deinen Clanprestige!"
			send " &8▪ &e/clan ranks &8&l» &7Bearbeite die Clanränge!"
			send " &8▪ &e/Taler &8&l» &7Zeigt deine momentanigen Taler!"
			send " &8▪ &e/cspawn &f<Base/Spawn> &8&l» &7Setze deinen Spawn!"
			send " "		
			send "          &8&m--&7&m--&e&m--&6&m--&8( &e&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
			stop
			

		arg-1 is "ranks":
			{clan::%player%.in} is not set:
				send "{@Prefix} Du bist kein Mitglied eines Clans!"
				stop
				
			arg-2 is not set:
				{rank1.name.%{clan::%player%.in}%} is not set:
					set {rank1.name.%{clan::%player%.in}%} to "&eRang 1"
				{rank2.name.%{clan::%player%.in}%} is not set:
					set {rank2.name.%{clan::%player%.in}%} to "&eRang 2"
				{rank3.name.%{clan::%player%.in}%} is not set:
					set {rank3.name.%{clan::%player%.in}%} to "&eRang 3"
				{rank4.name.%{clan::%player%.in}%} is not set:
					set {rank4.name.%{clan::%player%.in}%} to "&eRang 4"
				{rank5.name.%{clan::%player%.in}%} is not set:
					set {rank5.name.%{clan::%player%.in}%} to "&eRang 5"
				{rank6.name.%{clan::%player%.in}%} is not set:
					set {rank6.name.%{clan::%player%.in}%} to "&eRang 6"
				send "          &8&m--&7&m--&e&m--&6&m--&8( &6&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
				send ""
				send "&7Rang 1: &r%colored {rank1.name.%{clan::%player%.in}%}%"
				send "&7Rang 2: &r%colored {rank2.name.%{clan::%player%.in}%}%"
				send "&7Rang 3: &r%colored {rank3.name.%{clan::%player%.in}%}%"
				send "&7Rang 4: &r%colored {rank4.name.%{clan::%player%.in}%}%"
				send "&7Rang 5: &r%colored {rank5.name.%{clan::%player%.in}%}%"
				send "&7Rang 6: &r%colored {rank6.name.%{clan::%player%.in}%}%"
				send ""
				send "&7Bearbeite die Namen mit &e/clan ranks &f<1/2/3> <Name>"
				send ""
				send "          &8&m--&7&m--&e&m--&6&m--&8( &6&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
				
				if {clan::%{clan::%player%.in}%.owner} is player:
					send "&7Dein Rang: &cLEITER"
					stop
				if {clan::rank.%{clan::%player%.in}%.%player%} is "1":
					send "&7Dein Rang: %colored {rank1.name.%{clan::%player%.in}%}%"
					stop
				if {clan::rank.%{clan::%player%.in}%.%player%} is "2":
					send "&7Dein Rang: %colored {rank2.name.%{clan::%player%.in}%}%"
					stop
				if {clan::rank.%{clan::%player%.in}%.%player%} is "3":
					send "&7Dein Rang: %colored {rank3.name.%{clan::%player%.in}%}%"
					stop
				if {clan::rank.%{clan::%player%.in}%.%player%} is "4":
					send "&7Dein Rang: %colored {rank4.name.%{clan::%player%.in}%}%"
					stop
				if {clan::rank.%{clan::%player%.in}%.%player%} is "5":
					send "&7Dein Rang: %colored {rank5.name.%{clan::%player%.in}%}%"
					stop
				if {clan::rank.%{clan::%player%.in}%.%player%} is "6":
					send "&7Dein Rang: %colored {rank6.name.%{clan::%player%.in}%}%"
					stop
					
					
					
			arg-2 is not "1" or "2" or "3" or "4" or "5" or "6":
				make player execute command "/clan ranks"
				stop
				
			arg-2 is "1":
				arg-3 is not set:
					send "{@Prefix} Gebe den Namen des Spielers ein."
					stop
				else:
					if {clan::%{clan::%player%.in}%.owner} is player:
						set {rank1.name.%{clan::%player%.in}%} to "%arg-3%"
						send "{@Prefix} &eRang 1 nennt sich nun: %{rank1.name.%{clan::%player%.in}%}%"
						stop
					send "{@Prefix} Du musst der Besitzer des Clans sein."
					stop
					
			arg-2 is "2":
				arg-3 is not set:
					send "{@Prefix} Gebe den Namen des Spielers ein."
					stop
				else:
					if {clan::%{clan::%player%.in}%.owner} is player:
						set {rank2.name.%{clan::%player%.in}%} to "%arg-3%"
						send "{@Prefix} Rang 2 nennt sich nun: %{rank2.name.%{clan::%player%.in}%}%"
						stop
					send "{@Prefix} Du musst der Besitzer des Clans sein."
					stop
					
			arg-2 is "3":
				arg-3 is not set:
					send "{@Prefix} Gebe den Namen des Spielers ein."
					stop
				else:
					if {clan::%{clan::%player%.in}%.owner} is player:
						set {rank3.name.%{clan::%player%.in}%} to "%arg-3%"
						send "{@Prefix} Rang 3 nennt sich nun: %{rank3.name.%{clan::%player%.in}%}%"
						stop
					send "{@Prefix} Du musst der Besitzer des Clans sein."
					stop
					
			arg-2 is "4":
				arg-3 is not set:
					send "{@Prefix} Gebe den Namen des Spielers ein."
					stop
				else:
					if {clan::%{clan::%player%.in}%.owner} is player:
						set {rank4.name.%{clan::%player%.in}%} to "%arg-3%"
						send "{@Prefix} Rang 4 nennt sich nun: %{rank4.name.%{clan::%player%.in}%}%"
						stop
					send "{@Prefix} Du musst der Besitzer des Clans sein."
					stop
					
			arg-2 is "5":
				arg-3 is not set:
					send "{@Prefix} Gebe den Namen des Spielers ein."
					stop
				else:
					if {clan::%{clan::%player%.in}%.owner} is player:
						set {rank5.name.%{clan::%player%.in}%} to "%arg-3%"
						send "{@Prefix} Rang 5 nennt sich nun: %{rank5.name.%{clan::%player%.in}%}%"
						stop
					send "{@Prefix} Du musst der Besitzer des Clans sein."
					stop
					
			arg-2 is "6":
				arg-3 is not set:
					send "{@Prefix} Gebe den Namen des Spielers ein."
					stop
				else:
					if {clan::%{clan::%player%.in}%.owner} is player:
						set {rank6.name.%{clan::%player%.in}%} to "%arg-3%"
						send "{@Prefix} &eRang 7 nennt sich nun: %{rank6.name.%{clan::%player%.in}%}%"
						stop
					send "{@Prefix} Du musst der Besitzer des Clans sein."
					stop
					
					
					
					
		arg-1 is "promote":
			arg-2 is not set:
				send "{@Prefix} Bitte gib den Namen des Mitglieds an!"
				stop
			else:
				arg-2 is "%player%":
					send "{@Prefix} Du kannst dich nicht promoten du Schlingel :)"
					stop
				arg-3 is not set:
					send "{@Prefix} Bitte gib den Rang an welchen du verteilen möchtest!"
					stop
				arg-3 is not "1", "2", "3", "4", "5" or "7":
					send "{@Prefix} Du kannst einen Rang zwischen 1 und 7 wählen!"
					stop
				arg-3 is "1", "2", "3", "4", "5" or "7":
					loop all players:
						loop-player is arg-2:
							{clan::%player%.in} is {clan::%loop-player%.in}:
								{clan::rank.%{clan::%loop-player%.in}%.%loop-player%} is not set:
									set {clan::rank.%{clan::%loop-player%.in}%.%loop-player%} to 1
								{clan::%{clan::%player%.in}%.owner} is player:
									send "%{clan::rank.%{clan::%loop-player%.in}%.%loop-player%}%"
									{clan::rank.%{clan::%loop-player%.in}%.%loop-player%} is "6":
										set {clan::rank.%{clan::%loop-player%.in}%.%loop-player%} to arg-3
										send "{@Prefix} Du hast den Spieler %arg-2% befördert!"
										send "{@Prefix} Du wurdest befördert! Du bist nun Rang %{clan::rank.%{clan::%loop-player%.in}%.%loop-player%}%" to loop-player
										stop
									{clan::rank.%{clan::%loop-player%.in}%.%loop-player%} is "5":
										set {clan::rank.%{clan::%loop-player%.in}%.%loop-player%} to arg-3
										send "{@Prefix} Du hast den Spieler %arg-2% befördert!"
										send "{@Prefix} Du wurdest befördert! Du bist nun Rang %{clan::rank.%{clan::%loop-player%.in}%.%loop-player%}%" to loop-player
										stop
									{clan::rank.%{clan::%loop-player%.in}%.%loop-player%} is "4":
										set {clan::rank.%{clan::%loop-player%.in}%.%loop-player%} to arg-3
										send "{@Prefix} Du hast den Spieler %arg-2% befördert!"
										send "{@Prefix} Du wurdest befördert! Du bist nun Rang %{clan::rank.%{clan::%loop-player%.in}%.%loop-player%}%" to loop-player
										stop
									{clan::rank.%{clan::%loop-player%.in}%.%loop-player%} is "3":
										set {clan::rank.%{clan::%loop-player%.in}%.%loop-player%} to arg-3
										send "{@Prefix} Du hast den Spieler %arg-2% befördert!"
										send "{@Prefix} Du wurdest befördert! Du bist nun Rang %{clan::rank.%{clan::%loop-player%.in}%.%loop-player%}%" to loop-player
										stop
									{clan::rank.%{clan::%loop-player%.in}%.%loop-player%} is "2":
										set {clan::rank.%{clan::%loop-player%.in}%.%loop-player%} to arg-3
										send "{@Prefix} Du hast den Spieler %arg-2% befördert!"
										send "{@Prefix} Du wurdest befördert! Du bist nun Rang %{clan::rank.%{clan::%loop-player%.in}%.%loop-player%}%" to loop-player
										stop
									{clan::rank.%{clan::%loop-player%.in}%.%loop-player%} is "1":
										set {clan::rank.%{clan::%loop-player%.in}%.%loop-player%} to arg-3
										send "{@Prefix} Du hast den Spieler %arg-2% befördert!"
										send "{@Prefix} Du wurdest befördert! Du bist nun Rang %{clan::rank.%{clan::%loop-player%.in}%.%loop-player%}%" to loop-player
										stop
									send "{@Prefix} Der Spieler kann nur maximal Rang 7 sein."
									stop
								send "{@Prefix} Du bist nicht der Besitzer des Clans."
								stop
							send "{@Prefix} Der Spieler ist nicht in deinem Clan!"
							stop
							
							
							
							
		arg-1 is "demote":
			arg-2 is not set:
				send "{@Prefix} Bitte gib den Namen des Mitglieds an!"
				stop
				
			else:
				arg-2 is "%player%":
					send "{@Prefix} Du kannst dich nicht demoten du Schlingel :)"
					stop
				loop all players:
					loop-player is arg-2:
						{clan::%player%.in} is {clan::%loop-player%.in}:
							{clan::rank.%{clan::%loop-player%.in}%.%loop-player%} is not set:
								set {clan::rank.%{clan::%loop-player%.in}%.%loop-player%} to 1
							{clan::%{clan::%player%.in}%.owner} is player:
								{clan::rank.%{clan::%loop-player%.in}%.%loop-player%} is more than 1:
									remove 1 from {clan::rank.%{clan::%loop-player%.in}%.%loop-player%}
									send "{@Prefix} Du hast den Spieler %arg-2% degradiert!"
									send "{@Prefix} Du wurdest degradiert! Du bist nun Rang %{clan::rank.%{clan::%loop-player%.in}%.%loop-player%}%" to loop-player
									stop
								send "{@Prefix} Der Spieler kann nur minimal Rang 1 sein."
								stop
							send "{@Prefix} Du bist nicht der Besitzer des Clans."
							stop
						send "{@Prefix} Der Spieler ist nicht in deinem Clan!"
						stop
						
						
						
						
						
						
		arg-1 is "prestige":
			arg-2 is not set:
				send "          &8&m--&7&m--&e&m--&6&m--&8( &6&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
				send ""
				send "&7Das Clanprestige macht deinem Clan mehr macht."
				send "&7Je höher das Prestige ist, desto mehr Macht besitzt ihr!"
				send "&7Das Prestige kann durch die Clankasse erweitert werden."
				send ""
				send "&e/clan prestige info &8&l» Zeige die Info für deinen Clan!"
				send ""
				send "          &8&m--&7&m--&e&m--&6&m--&8( &6&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
				stop
				
				
				
				
				
				
			arg-2 is "info":
				arg-3 is not set:
					{clan::%player%.in} is not set:
						send "{@Prefix} Du bist in keinem Clan!"
						stop
					{clan::%{clan::%player%.in}%.prestige} is not set:
						set {clan::%{clan::%player%.in}%.prestige} to 0
						set {clan::%{clan::%player%.in}%.prestige.pay} to 1000000
						send "          &8&m--&7&m--&e&m--&6&m--&8( &6&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
						send ""
					{clan::%{clan::%player%.in}%.prestige} is more than or equal to 25:
						set {clan::%{clan::%player%.in}%.prestige} to 25
						set {clan::%{clan::%player%.in}%.prestige.pay} to 0
					send " "
					send "&7Das jetzige Clanprestige: &7%{clan::%{clan::%player%.in}%.prestige}%"
					send "&7Kosten für Erweiterung: &7%{clan::%{clan::%player%.in}%.prestige.pay}%"
					send "&e/clan prestige kaufen &8&l» Erweitere dein Clanprestige!"
					send ""
					send "          &8&m--&7&m--&e&m--&6&m--&8( &6&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
					stop
					
				arg-3 is set:
					{clan::%arg-3%.owner} is set:
						{clan::%{clan::%arg-3%.in}%.prestige} is not set:
							set {clan::%{clan::%arg-3%.in}%.prestige} to 0
							set {clan::%{clan::%arg-3%.in}%.prestige.pay} to 1000000
						{clan::%{clan::%arg-3%.in}%.prestige} is more than or equal to 25:
							set {clan::%{clan::%arg-3%.in}%.prestige} to 25
							set {clan::%{clan::%arg-3%.in}%.prestige.pay} to 0
						send "          &8&m--&7&m--&e&m--&6&m--&8( &6&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
						send " "
						send "&e%arg-3%'s Clanprestige: &f%{clan::%arg-3%.prestige}%"
						send ""
						send "          &8&m--&7&m--&e&m--&6&m--&8( &6&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
						stop
					send "{@Prefix} Dieser Clan existiert nicht!"
					stop
					
					
					
					
			arg-2 is "kaufen":
				{clan::%{clan::%player%.in}%.owner} is player:
					{clan::%{clan::%player%.in}%.Taler} is more than or equal to {clan::%{clan::%player%.in}%.prestige.pay}:
						{clan::%{clan::%player%.in}%.prestige} is less than 25:
							remove {clan::%{clan::%player%.in}%.prestige.pay} from {clan::%{clan::%player%.in}%.Taler}
							add 1000000 to {clan::%{clan::%player%.in}%.prestige.pay}
							add 1 to {clan::%{clan::%player%.in}%.prestige}
							send "{@Prefix} &eDu hast deinen Clan-Prestige um 1 erweitert!"
							wait 1 second
							broadcast " "
							broadcast "{@Prefix} &eClan &f%{clan::%player%.in}% &7ist nun im Prestige ##&f%{clan::%{clan::%player%.in}%.prestige}%&7!"
							broadcast "{@Prefix} Die Macht des Clans ist gewachsen!"
							broadcast " "
							stop
						send "{@Prefix} Dein Clan verfügt über das höchste Prestigelevel!"
						stop
					send "{@Prefix} Dein Clan verfügt nicht genügend &eTaler!"
					stop
				send "{@Prefix} Du bist nicht der Besitzer des Clans."
				stop
				
				
				
				
	
				
				
				
				
				
				
		arg-1 is "kasse":
			{clan::%{clan::%player%.in}%.Taler} is not set:
				set {clan::%{clan::%player%.in}%.Taler} to 0
			arg-2 is not set:
				send "          &8&m--&7&m--&e&m--&6&m--&8( &6&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
				send ""
				send " &eTaler &7deines &eClans: &e%{clan::%{clan::%player%.in}%.Taler}%"
				send " " 
				send " &e/clan kasse einzahlen &f<Taler>"
				send " "
				send " &7Zahle &eTaler &7in die &eClankasse &7ein!"
				send ""
				send "          &8&m--&7&m--&e&m--&6&m--&8( &6&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
				stop
			arg-2 is "einzahlen":
				arg-3 is not set:
					send "{@Prefix} &e/clan kasse einzahlen &f<Betrag>"
					stop
				make server execute command "ckeb %player% %arg-3%"
				stop
				
				
				
				
				
				
		if arg-1 is "hilfe":
			make player execute command "/clan"
			stop
			
			
			
			
			
			
		if arg-1 is "info":
			if arg-2 is not set:
				if {clan::%player%.status} is true:
					send "          &8&m--&7&m--&e&m--&6&m--&8( &6&lCLAN &8)&6&m--&e&m--&7&m--&8&m--"
					send ""
					send "&7Level des Clans: &e%{clan::%{clan::%player%.in}%.level}%"
					send "&7Taler des Clans: &e%{clan::%{clan::%player%.in}%.Taler}%"
					send "&7Leiter des Clans: &f%{clan::%{clan::%player%.in}%.owner}%"
					send "&7Spieler des Clans: &f%{clan::%{clan::%player%.in}%.Members}%&7/&7%{clan::%{clan::%player%.in}%.slots}%"
					send "&7Slots des Clans: &c%{clan::%{clan::%player%.in}%.slots}%"
					if {clan::%{clan::%player%.in}%.Motto} is set:
						send "&eClanmotto: &7%{clan::%{clan::%player%.in}%.Motto}%"
					send " "
