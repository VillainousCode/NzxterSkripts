command /beitreten [<text>]:
	trigger:
		if {clan::%player%.input} is true:
			if arg-1 is not "Ja" or "Nein":
				send "&7Tippe &e'/beitreten &eJa' um beizutreten."
				send "&7Tippe &e'/beitreten &cNein' um nicht beizutreten."
				stop
			else:
			
				if arg-1 is "Ja":
					Wait a tick
					make player execute command "/clan jddin %{clan::%player%.invited}%"
					send "{@Prefix} &f%player% hat die Einladung angenommen!" to {clan::%player%.inviter}
					set {clan::%player%.status} to true
					set {clan::%player%.in} to {clan::%player%.invited}
					add 1 to {clan::%{clan::%player%.in}%.Members}
					set {clan::rank.%{clan::%player%.in}%.%player%} to "1"
					
				if arg-1 is "Nein":
					wait a tick
					send "{@Prefix} Du hast die Einladung abgelehnt."
					delete {clan::%player%.delete}
					stop
				delete {clan::%player%.invited}
				delete {clan::%player%.input}
				delete {clan::%player%.inviter}

command /c [<text>]:
	trigger:
		if {clan::%player%.status} is true:
			{clan::rank.%{clan::%player%.in}%.%player%} is not set:
				set {clan::rank.%{clan::%player%.in}%.%player%} to "1"
				
			arg-1 is not set:
				send "{@Prefix} Bitte gib eine Nachricht ein!"
				stop
				
			loop all players:
				if {clan::%loop-player%.in} is {clan::%player%.in}:
					{clan::%{clan::%player%.in}%.owner} is player:
						send "&7%{clan::%player%.in}% &8&l» &cLEADER &8&l» &f%name of player%&7: &e%arg%" to loop-player
					else:
						{clan::rank.%{clan::%player%.in}%.%player%} is "1":
							send "&7%{clan::%player%.in}% &8&l» %colored {rank1.name.%{clan::%player%.in}%}% &8&l» &7%name of player%&7: &e%arg%" to loop-player
						{clan::rank.%{clan::%player%.in}%.%player%} is "2":
							send "&7%{clan::%player%.in}% &8&l» %colored {rank2.name.%{clan::%player%.in}%}% &8&l» &7%name of player%&7: &e%arg%" to loop-player
						{clan::rank.%{clan::%player%.in}%.%player%} is "3":
							send "&7%{clan::%player%.in}% &8&l» %colored {rank3.name.%{clan::%player%.in}%}% &8&l» &7%name of player%&7: &e%arg%" to loop-player
						{clan::rank.%{clan::%player%.in}%.%player%} is "4":
							send "&7%{clan::%player%.in}% &8&l» %colored {rank4.name.%{clan::%player%.in}%}% &8&l» &7%name of player%&7: &e%arg%" to loop-player
						{clan::rank.%{clan::%player%.in}%.%player%} is "5":
							send "&7%{clan::%player%.in}% &8&l» %colored {rank5.name.%{clan::%player%.in}%}% &8&l» &7%name of player%&7: &e%arg%" to loop-player
						{clan::rank.%{clan::%player%.in}%.%player%} is "7":
							send "&7%{clan::%player%.in}% &8&l» %colored {rank6.name.%{clan::%player%.in}%}% &8&l» &7%name of player%&7: &e%arg%" to loop-player

			log "%{clan::%player%.in}% %name of player%: %arg%" to "Clanlogs/chat.log"
			stop
		else:
			send "{@Prefix} Du bist kein Mitglied eines Clans!"
			stop

	
