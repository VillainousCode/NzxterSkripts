						stop
						
				if arg-1 is "Nein":
					wait a tick
					send "{@Prefix} Der Clan wurde nicht aufgelöst."
					delete {clan::%player%.delete}
					stop
	
	
command /ckeb [<player>] [<integer>]:
	executable by: console
	trigger:
		{clan::%arg player%.in} is set:
			arg-2 is less than 1:
				send "{@Prefix} Du musst &cmind. &71 &eTaler &7einzahlen!" to arg player
				stop
			set {_taler} to "%arg-2%" parsed as number
			if {_taler} is a number:
				if {_taler} is bigger than 0:
					if {Taler::%arg player%} is bigger than {_taler}-0.01:
						remove {_taler} from {Taler::%arg player%}
						add {_taler} to {clan::%{clan::%arg player%.in}%.Taler}
						send "{@Prefix} Du hast &e%{_taler}% &eTaler &7in die Clankasse eingezahlt." to arg player
						loop all players:
							{clan::%loop-player%.in} is {clan::%arg player%.in}:
								send "{@Prefix} &f%arg player% &7hat &e%{_taler}% &eTaler &7in die Clankasse eingezahlt." to loop-player
								stop
						stop
					send "{@Prefix} Du hast nicht genügend &eTaler!" to arg player
					stop
				send "{@Prefix} Du bist kein Mitglied eines Clans!" to arg player
				stop
		
		

		
