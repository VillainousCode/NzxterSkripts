options:	Prefix: &8&l【&6&lNzxterMC&8&l】&8&l▶&7variables:	{_clearlagmobs} = 0	{_clearlagitems} = 0	command /lag:	aliases: /clearlag, /cl	permission: server.team	trigger:		loop all entities in world "Nether", "End", "Freebuild", "Farmwelt":				if loop-entity is not a player or dropped items or a villager:				delete loop-entity				add 1 to {_clearlagmobs}		loop all dropped items in world "Nether", "End", "Freebuild", "Farmwelt":			delete loop-value			add 1 to {_clearlagitems}		broadcast "{@Prefix} Der &6Staubsauger &7hat &8(&4&l%{_clearlagitems}% &6Items &8/ &4&l%{_clearlagmobs}% &6Mobs&8) &7weggesaugt"			delete {_clearlagmobs}		delete {_clearlagitems}		stop 	every 57 minutes:	broadcast "{@Prefix} In &a&l30 &7Sekunden kommt der &6Staubsauger"	wait 25 seconds	broadcast "{@Prefix} In &a&l3 &7Sekunden kommt der &6Staubsauger"	wait 1 second	broadcast "{@Prefix} In &a&l2 &7Sekunden kommt der &6Staubsauger"	wait 1 second	broadcast "{@Prefix} In &a&l1 &7Sekunde kommt der &6Staubsauger"	wait 1 second	loop all entities in world "Nether", "End", "Freebuild", "Farmwelt":			if loop-entity is not a player or dropped items or a villager:			delete loop-entity			add 1 to {_clearlagmobs}	loop all dropped items in world "Nether", "End", "Freebuild", "Farmwelt":		delete loop-value		add 1 to {_clearlagitems}	broadcast "{@Prefix} Der &6Staubsauger &7hat &8(&4&l%{_clearlagitems}% &6Items &8/ &4&l%{_clearlagmobs}% &6Mobs&8) &7weggesaugt"		delete {_clearlagmobs}	delete {_clearlagitems}	stop 	